<div class="container-fluid">
    <div class="row">
      <h2>Game score</h2>
    </div>
    <div class="row">
      <div
        class="card"
        style="max-width: 18rem"
        v-for="player in leaderboard.keys()"
      >
        <div class="card-body">
          <h5 class="card-title">{{ player }}</h5>
          <p class="card-text">Score: {{ leaderboard.get(player) }} points.</p>
        </div>
      </div>
    </div>
  </div>
  